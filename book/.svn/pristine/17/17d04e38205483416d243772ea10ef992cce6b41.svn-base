<?xml version="1.0" encoding="UTF-8"?>
<excel id="society" code="society" name="社会绘本信息表">
    <colgroup>
        <col index="A" width='30em'></col>
        <col index="B" width='17em'></col>
        <col index="C" width='17em'></col>
        <col index="D" width='60em'></col>
    </colgroup>
    <title>
        <tr height="30px">
            <td rowspan="1" colspan="4" value="社会绘本信息表" />
        </tr>
    </title>
    <thead>
        <tr height="30px">
            <th value="绘本名" />
            <th value="绘本所属班级" />
            <th value="绘本主题" />
            <th value="绘本描述" />
        </tr>
    </thead>
    <tbody>
        <tr height="30px" firstrow="2" firstcol="0" repeat="5">
            <td type="string" isnullable="false" maxlength="50" /><!--绘本名 -->
            <td type="enum" format="大班,中班,小班" isnullable="true" /><!--绘本所属班级 -->
            
            <td type="string" isnullable="false" maxlength="30" /><!--绘本主题 -->
            <td type="string" isnullable="false" maxlength="100" /><!--绘本描述 -->
            
        </tr>
    </tbody>
</excel>